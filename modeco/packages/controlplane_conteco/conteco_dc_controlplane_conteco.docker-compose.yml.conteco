---
version: '3'
services:

# START SERVICE controlplane.conteco
  controlplane:
    image: ${CONTECO_REGISTRY}${CONTECO_REALM_RUNTIME}/${CONTECO_ECOSYSTEM_RUNTIME}.controlplane.conteco:v2.0.8 
    environment: [${CONTECO_DC_CONTROLPLANE_CONTECO_ENVIRONMENT}]
    command: [${CONTECO_DC_CONTROLPLANE_CONTECO_COMMAND}]
    volumes: ['/etc:/hostfs/etc:ro','/var/run/docker.sock:/var/run/docker.sock','controlplane_repos_${CONTECO_DC_GLOBAL_STACKNAMES}:/conteco/pwd']
    ports: []
    networks: []
    deploy:
      mode: ${CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_MODE}
      resources:
        limits:
          memory: '${CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_LIMITS_MEMORY}'
        reservations:
          memory: '${CONTECO_DC_CONTROLPLANE_CONTECO_DEPLOY_RESOURCES_RESERVATIONS_MEMORY}'
${CONTECO_DC_CONTROLPLANE_CONTECO_COMPOSE_EXTENSIONS}
# END SERVICE controlplane.conteco

networks: {}
volumes: {controlplane_repos_${CONTECO_DC_GLOBAL_STACKNAMES}: { external: true }}
${CONTECO_DC_GLOBAL_COMPOSE_EXTENSIONS}
